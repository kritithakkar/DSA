public class Main
{

public static void delete(int arr[],int pos)
{
    for(int i=pos-1;i<arr.length-1;i++)
    {
        arr[i] = arr[i+1];
     }
}
public static void main(String args[])
{
    int arr[] = {1,2,3,4,5,6};
    int pos = 3;
    delete(arr,pos);
    for(int i=0;i<arr.length-1;i++)
    {
        System.out.print(arr[i]+" ");
    }
}
}
